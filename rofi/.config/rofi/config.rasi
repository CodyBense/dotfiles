configuration {
    modes: [drun,filebrowser,run];
    show-icons: true;
    icon-theme: "Papirus";
    location: 0;
    font: "JetBrainsMono Nerd Font Mono 12";
    drun-display-format: "{icon} {name}";
    display-drun: " Apps";
    display-run: " Run";
    display-filebrowser: " File";
}

@theme "theme"
